---
import { navLinks } from "../data/NavLists";
---
<nav class="md:ml-auto md:mr-auto flex flex-wrap items-center text-base justify-center">
  <ul class="flex">
    {
      navLinks.map(({ label, href }) => (
        <li class="mr-5 hover:text-gray-900 text-xl">
          <a href={href}>
            {label}
          </a>
        </li>
      ))
    }
  </ul>
</nav>